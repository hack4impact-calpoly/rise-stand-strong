{
   "env": {
      "browser": true,
      "es2021": true
   },
   "extends": [
      "plugin:react/recommended",
      "plugin:prettier/recommended",
      "airbnb"
   ],
   "parserOptions": {
      "ecmaFeatures": {
         "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
   },
   "plugins": ["react"],
   "rules": {
      "import/no-extraneous-dependencies": [
         "error",
         {
            "devDependencies": true
         }
      ]
   },
   "settings": {
      "import/resolver": {
         "node": {
            "extensions": [".js", ".jsx", ".ts", ".tsx"]
         }
      }
   }
}
